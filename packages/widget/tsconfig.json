{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "emitDeclarationOnly": false,
    "module": "esnext",
    "preserveSymlinks": false,
    "declaration": true,
    "noEmit": false,
    "declarationDir": "dist",
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true 
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
  ]
}

