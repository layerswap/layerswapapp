{
    "jsc": {
        
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "development": false
            }
        },
        "target": "es2020"
    },
    "module": {
        "type": "es6",
        "strict": true
    },
    "sourceMaps": true
}